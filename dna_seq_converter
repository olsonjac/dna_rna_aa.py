file = open('sample_dna.txt', 'r')
dna = file.read().upper()
print ("DNA: ", dna)

rna = ""

# Generate the RNA string
for i in dna:
    # Replace all occurrences of T with U
    if i == "T":
      rna += "A"
    elif i == "A":
      rna += "U"
    elif i == "C":
      rna += "G"
    elif i == "G":
      rna += "C"    
    else:
        rna += i

# Print the RNA string
print( "RNA: ", rna)
